	"license": "MIT",
	"devDependencies": {
		"@types/express": "^5.0.5",
		"@types/node": "^24.10.1",
		"@typescript-eslint/eslint-plugin": "^8.48.0",
		"@typescript-eslint/parser": "^8.48.0",
		"@vitest/coverage-v8": "^4.0.14",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.0",
		"happy-dom": "^20.0.11",
		"obsidian": "latest",
		"tslib": "^2.8.1",
		"typescript": "^5.0.0",
		"vitest": "^4.0.14"
	},
	"dependencies": {
		"@agentclientprotocol/sdk": "^0.5.1",
		"@codemirror/merge": "^6.11.0",
