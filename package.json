{
	"name": "acp-client",
	"version": "1.0.0",
	"description": "An Obsidian plugin that acts as an Agent Client Protocol (ACP) client",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"lint": "eslint ."
	},
	"keywords": [],
	"author": "Jacob Mikesell",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@eslint/json": "0.14.0",
		"@types/express": "^5.0.5",
		"@types/node": "^24.10.1",
		"@vitest/coverage-v8": "^4.0.14",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.0",
		"eslint": "^9.17.0",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"happy-dom": "^20.0.11",
		"obsidian": "latest",
		"tslib": "^2.8.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.48.0",
		"vitest": "^4.0.14"
	},
	"dependencies": {
		"@agentclientprotocol/sdk": "^0.9.0",
		"@codemirror/merge": "^6.11.0",
		"@modelcontextprotocol/sdk": "^1.23.1",
		"express": "^5.1.0",
		"shell-env": "^4.0.1",
		"zod": "^4.1.13"
	}
}
